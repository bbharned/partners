<div class="container">

<%= render 'shared/errors', obj: @tmversion %>

<%= form_for(@tmversion, :html => {class: "form-horizontal", role: "form"}) do |f| %>

<div class="col-md-4">

            <div class="form-group">
                <div class="row">
                    <%= f.label :version %>
                </div>
                <div class="row">
                    <%= f.text_field :version, class: "form-control", placeholder: "Enter ThinManager Version Number", autofocus: true %>
                </div>
            </div>
       

        <br />
        	<div class="row">
	            <%= f.submit(@tmversion.new_record? ? "Create TM Version" : "Update TM Version", class: "btn btn-primary btn-lg") %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= link_to "Cancel", :back, style: "padding-top:12px;" %>
	        </div>
    <% end %>
    




</div>